[Defines]
  INF_VERSION                    = 0x00010005
  BASE_NAME                      = SharedCryptoDxeLoader
  FILE_GUID                      = 4F25834D-11D0-4A8F-9860-72B303B9F242
  MODULE_TYPE                    = DXE_DRIVER
  VERSION_STRING                 = 1.0
  ENTRY_POINT                    = DxeEntryPoint

#
# The following information is for reference only and not required by the build tools.
#
#  VALID_ARCHITECTURES           = IA32 X64 ARM AARCH64

[Sources]
  SharedCryptoLoaderDxe.c
  SharedLoaderShim.h
  PeCoffLib.h
  PeCoffLib.c

[Packages]
  MdePkg/MdePkg.dec
  MdeModulePkg/MdeModulePkg.dec
  OpensslPkg/OpensslPkg.dec

[LibraryClasses]
  UefiDriverEntryPoint
  DebugLib
  BaseMemoryLib
  MemoryAllocationLib
  DxeServicesLib
  PeCoffGetEntryPointLib
  PeCoffLib
  CacheMaintenanceLib
  DxeServicesTableLib
  UefiBootServicesTableLib
  SafeIntLib
  RngLib

[Protocols]
  gEfiMemoryAttributeProtocolGuid     ## CONSUMES
  gSharedCryptoDxeProtocolGuid        ## PRODUCES
  gEfiRngProtocolGuid                 ## CONSUMES

[Depex]
  TRUE
  
# Using TRUE DEPEX with lazy RNG initialization
# RNG protocol will be located on first use rather than at driver load time
# This prevents boot hangs when RNG protocol is not immediately available